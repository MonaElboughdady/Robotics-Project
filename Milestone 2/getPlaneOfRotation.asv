function planeOfRotation = getPlaneOfRotation(X1,X2,X3)
syms a b c d
eqn1 = X1(1)*a+X1(2)*b+X1(3)*c+d == 0;
eqn2 = a*X2(1)+b*X2(2)+c*X2(3)+d == 0;
eqn3 = a*X3(1)+b*X3(2)+c*X3(3)+d == 0;
Coefficie = solve(subs([eqn1 eqn2 eqn3],a,1),[b c d]);
planeOfRotation = [1, subs(Coefficients(1),a,1), subs(Coefficients(2),a,1), subs(Coefficients(3),a,1)];
end